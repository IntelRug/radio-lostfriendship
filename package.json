    "apollo-link-error": "^1.1.13",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "2",
    "graphql-tag": "^2.10.3",
    "leaflet": "^1.6.0",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.9.1",
    "nuxt-leaflet": "^0.0.21",
    "nuxt-property-decorator": "^2.5.1",
    "nuxt-typed-vuex": "^0.1.20",
    "typed-vuex": "^0.1.20",
    "universal-cookie": "^4.0.3",
    "vue": "^2.6.11",
