    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "jest": "^24.1.0",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.3",
    "ts-jest": "^25.0.0",
    "vue-jest": "^4.0.0-0"
  }
